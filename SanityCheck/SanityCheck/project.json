{
  "scripts": {
    "postcompile": [
      "dotnet pack --no-build --configuration %compile:Configuration%"
    ]
  },
  "version": "1.0.0.0",
  "name": "SanityCheck",
  "title": "SanityCheck",
  "description": "Xamarin.Forms \"sanity check\" project that tests compatibility between latest version of several NuGet packages",
  "language": "en-US",
  "authors": [
    "Thomas Hagström",
    "HL interactive"
  ],
  "embedInteropTypes": true,
  "copyright": "",
  "packOptions": {
    "summary": "Xamarin.Forms sanity check of NuGet packages",
    "releaseNotes": "Multi-target NetStandard 1.4, .Net 4 and PCL",
    "projectUrl": "https://github.com/HLinteractive/SanityCheck",
    "tags": [
      "nuget",
      "pcl",
      "xamarin",
      "xamarin.forms"
    ],
    "licenseUrl": "https://github.com/HLinteractive/SanityCheck/raw/master/LICENSE",
    "iconUrl": "https://dl.dropboxusercontent.com/u/1027259/hliicon.ico",
    "requireLicenseAcceptance": true,
    "repository": {
      "type": "git",
      "url": "git://github.com/HLinteractive/SanityCheck.git"
    }
  },
  "supports": {},
  "frameworks": {
    ".NETPortable,Version=v4.5,Profile=Profile111": {
      "buildOptions": {
        "define": [
          "PORTABLE"
        ],
        "warningsAsErrors": false
      },
      "frameworkAssemblies": {
        "Microsoft.CSharp": "",
        "System.Collections": "",
        "System": "",
        "System.Core": ""
      }
    },
    "net45": {
      "frameworkAssemblies": {}
    },
    "net46": {
      "frameworkAssemblies": {}
    },
    ".NETStandard,Version=v1.4": {
      "imports": "portable-net45+win8+wpa81",
      "buildOptions": {
        "warningsAsErrors": false,
        "define": [
          "NETSTANDARD1_4"
        ]
      },
      "dependencies": {
        "Microsoft.CSharp": "4.3.0",
        "NETStandard.Library": "1.6.1"
      }
    }
  },
  "dependencies": {
    "CommonServiceLocator": "1.3.0",
    "HLI.Core": "1.0.5.3",
    "Newtonsoft.Json": "10.0.2",
    "Prism.Core": "6.3.0",
    "Prism.Forms": "6.3.0",
    "Prism.Unity.Forms": "6.3.0",
    "Syncfusion.Xamarin.SfChart": "15.1.0.41",
    "Syncfusion.Xamarin.SfDataGrid": "15.1.0.41",
    "Unity": "4.0.1",
    "Xamarin.Forms": "2.3.4.224"
  }
}